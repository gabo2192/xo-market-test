# Copy this to terraform.tfvars and fill in your values

project_name = "xo-market"
environment  = "dev"
aws_region   = "us-east-1"

# Database Configuration
database_name = "xo_market_db"
db_username   = "xo_market_user"
db_password   = "your-secure-database-password"

# Redis Configuration
redis_password = "your-secure-redis-password"

# Container Images (update after building and pushing to ECR)
api_container_image    = "123456789012.dkr.ecr.us-east-1.amazonaws.com/xo-market-dev-api:latest"
worker_container_image = "123456789012.dkr.ecr.us-east-1.amazonaws.com/xo-market-dev-worker:latest"

# Application Configuration
api_port              = 3001
worker_port           = 3002
api_desired_count     = 1
worker_desired_count  = 1

# CORS Configuration
cors_origin = "https://your-frontend-domain.com"  # Update for production

# AI API Keys (optional - leave empty if not using AI features)
openai_api_key    = ""
anthropic_api_key = ""